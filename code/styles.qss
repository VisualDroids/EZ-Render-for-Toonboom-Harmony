* {
  color: #e8ebec;
  font-family: Roboto;
  font-size: 12px;
}
QWidget {
  background-color: #12111e;
}
QGroupBox,
QFrame {
  /* color: #e8ebec; */
  background-color: #1a192c;
  border: none;
  border-radius: 10px;
}
QGroupBox {
  padding-top: 20px;
}
QGroupBox::title {
  subcontrol-origin: margin;
  subcontrol-position: top center;
  padding-top: 20px;
}

QTabBar::tab {
  qproperty-alignment: AlignCenter;
}

QPushButton {
  background-color: #3b2775;
  border-radius: 6px;
  min-width: 40px;
  border: none;
}
QPushButton:hover {
  background-color: #6644c4;
  border: 0px;
  border-radius: 6px;
}
QPushButton:pressed {
  background-color: #281a52;
}
QPushButton:flat {
  background-color: #673dd8;
  border-radius: 6px;
}
QPushButton:flat {
  background-color: #773592;
}
QPushButton:flat:hover {
  background-color: #b739e9;
}
QLineEdit {
  /* color: #e8ebec;
  border: 2px solid #8459ff;
  border-width: 1px; */
  background-color: #1f1f32;
  border-radius: 6px;
  border: 0px solid;
  selection-background-color: #8459ff;
  selection-color: #e8ebec;
  min-height: 25px;
}
QLineEdit[text="\\"] {
  color: black;
}
QLineEdit:focus {
  /* color: #e8ebec; */
  border: 2px solid #8459ff;
  border-width: 1px;
  border-radius: 6px;
}
QTableWidget {
  border-radius: 10px;
  gridline-color: #1f1f32;
  background-color: #12111e;
  border: 1px solid #12111e;
}

QHeaderView {
  /* background: #e6e6e6; */
  border: none;
  /* min-height: 30px; */
  /* font-size: "Microsoft YaHei"; */
  /* font-size: 14px; */
}
QHeaderView::section:horizontal {
  border: none;
  /* color: #333333; */
  background: transparent;
  /* border-radius: 10px; */
  /* padding-left: 5px; */
}
QHeaderView::section:vertical {
  /* background: #ffffff; */
  border: none;
  background: transparent;
  /* padding-left: 5px; */
  text-align: center;
}

