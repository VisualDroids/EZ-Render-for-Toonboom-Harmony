/* Scrollbars */
QScrollBar {
  width: 10px;
  height: 10px;
}

QScrollArea {
  background: transparent;
}

/* QTableView QScrollBar {
  width: 10px;
  height: 3px;
} */

QScrollBar::handle {
  background: #999999;
  min-height: 30px;
  border-radius: 5px;
}

QScrollBar::handle:hover {
  background: #673dd8;
}
QScrollBar::handle:pressed {
  background: #431fa4;
}
QScrollBar::handle:disabled {
  background: #3d3737;
}

